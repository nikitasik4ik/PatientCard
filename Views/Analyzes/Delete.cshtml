@model PatientCard.Models.Analyze

@{
    ViewData["Title"] = "Удалить";
}

<h1>Удалить</h1>
<h3>Вы уверены, что хотите удалить этот элемент?</h3>
<div>
    <h4>Анализ</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Departament)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Departament.NameDepartament)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Organization)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Organization.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Service)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Service.NameService)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateAnalyzes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateAnalyzes)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Doctor)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Doctor.FullNameDoctor)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.FirstName) @Html.DisplayFor(model => model.User.LastName)
        </dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdAnalyzes" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Вернуться к списку</a>
    </form>
</div>