@model IEnumerable<PatientCard.Models.Analyze>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<p>
    <a class="btn btn-primary" asp-action="Create">Create New</a>
</p>
<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Department</th>
            <th scope="col">Organization</th>
            <th scope="col">Service</th>
            <th scope="col">DateAnalyzes</th>
            <th scope="col">Doctor</th>
            <th scope="col">User</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Departament.IdDepartament)</td>
                <td>@Html.DisplayFor(modelItem => item.Organization.IdOrganization)</td>
                <td>@Html.DisplayFor(modelItem => item.Service.IdService)</td>
                <td>@Html.DisplayFor(modelItem => item.DateAnalyzes)</td>
                <td>@Html.DisplayFor(modelItem => item.Doctor.IdDoctor)</td>
                <td>@Html.DisplayFor(modelItem => item.User.Id)</td>
                <td>
                    <div class="btn-group" role="group">
                        <a class="btn btn-info" asp-action="Details" asp-route-id="@item.IdAnalyzes">Details</a>
                        <a class="btn btn-warning" asp-action="Edit" asp-route-id="@item.IdAnalyzes">Edit</a>
                        <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.IdAnalyzes">Delete</a>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>